<idea-plugin package="com.jetbrains.edu.ai.clippy.assistant">
  <resource-bundle>messages.EduAIClippyAssistantBundle</resource-bundle>

  <dependencies>
    <!--suppress PluginXmlValidity -->
    <module name="AI"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <applicationService serviceImplementation="com.jetbrains.edu.ai.clippy.assistant.settings.AIClippySettings"/>
  </extensions>

  <extensions defaultExtensionNs="Educational">
    <AIOptionsProvider instance="com.jetbrains.edu.ai.clippy.assistant.settings.AIClippyOptions"/>
  </extensions>
</idea-plugin>