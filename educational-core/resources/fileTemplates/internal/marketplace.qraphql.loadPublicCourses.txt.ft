query {
  plugins(
    search: {
      filters: [{ field: "family", value: "edu", field: "fields.isPrivate", value: false}]
      max: ${max}
      offset: ${offset}
      sortBy: RATING
    }
  ) {
    total
    plugins {
      id
      name
      description
      downloads
      rating
      authors {
        name
      }
      organization {name}
      fields
      lastUpdateDate
      cdate
      link
      license
    }
  }
}