query {
  plugins(
    search: {
      filters: [{ field: "family", value: "edu"}, {field: "tags", value: "Early-bird"},  {field: "fields.isPrivate", value: "true"}]
      max: ${max}
      offset: ${offset}
      sortBy: RATING
    }
  ) {
    total
    plugins {
      id
      name
      description
      downloads
      rating
      developers
      organization {name}
      fields
      lastUpdateDate
      cdate
    }
  }
}